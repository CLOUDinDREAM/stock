/* pages/demo/demo.wxss */
.container{
  width: 100%;
  position: relative;
}
.demo-top{
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 10%;
  background-color: pink;
  z-index: 100;
}
.demo-middle{
  position: fixed;
  top: 10%;
  bottom: 10%;
  left: 0;
  width: 100%;
  height: 80%;
  background-color: purple;
  overflow: hidden;
}
.middle{
  width: 100%;
  height: 100%;
  overflow: scroll;
}
.middle button{
  width: calc(100%/3);
  float: left;

}
.demo-bottom{
  position: fixed;
  bottom: 0;
  left: 0;
  background-color: lightblue;
  height: 10%;
  width: 100%;
}
/* pages/list/list.wxss */
.container {
  width: 100%;
  position: relative;
}
.section-search {
  position: fixed;
  top: 0;
  left: 0;
  display: flex;
  align-items: center;
  justify-content: center;
  width: 100%;
  height: 10%;
}
.section-main {
  position: fixed;
  top: 10%;
  bottom: 10%;
  left: 0;
  width: 100%;
  height: 70%;
  overflow: hidden;
}
.section-bottom {
  position: fixed;
  bottom: 0;
  left: 0;
  width: 100%;
}
.main{
  width: 100%;
  height: 90%;
  overflow: scroll;
}
.total{
  width: 100%;
  height: 10%;
}
.main-for{
  height: 10%;
  width: 100%;
  border-bottom: 1px solid rgba(131, 223, 226, 0.486);

}
.main-for button{
  top: 5%;
  height: 90%;
  width: calc(100%/3);
  float: left;
  overflow: hidden;
  font-size: 30rpx;
  background-color:white;
  text-align: left;
}
.main-for button::after{ 
  border: none; 
}
/** 修改默认的navigator点击态 **/
.navigator-hover {
  color: rgb(255, 0, 34);
}

/** 自定义其他点击态样式类 **/

.other-navigator-hover {
  color: red;
}

.section-search input {
  border: 1px solid rgba(109, 148, 73, 0.199);
  border-radius: 12rpx;
  height: 1cm;
  width: 90%;
}
